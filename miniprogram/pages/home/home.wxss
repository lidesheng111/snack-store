/* themes */
.section-header {
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    color: #AB956D;
    font-size: 32rpx;
}

.theme-box {
    display: grid;
    grid-template-columns: 1fr 1fr;
    grid-gap: 5rpx;
}
    .themes-img:last-child {
        grid-column: 1/3;
    }

